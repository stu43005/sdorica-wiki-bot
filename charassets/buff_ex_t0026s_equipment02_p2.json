        "效果": [
          {
            "效果": [
              "技能的(Attack)屬性效果增加(15)%"
            ],
            "條件": [
              "施術者是本角色"
            ]
          },
          {
            "效果": [
              "技能效果增加((30 * 在施術者的bonusAttackPower(強化)狀態的層數))%"
